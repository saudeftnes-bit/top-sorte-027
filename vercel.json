{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": null,
    "alias": [
        "topsorte-027.vercel.app"
    ],
    "rewrites": [
        {
            "source": "/api/efi-webhook/pix",
            "destination": "/api/efi-webhook"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/cron-cleanup",
            "schedule": "*/5 * * * *"
        }
    ]
}